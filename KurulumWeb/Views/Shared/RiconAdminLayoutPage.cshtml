    <script>

       // Inwi ve Orange checkbox'larının seçili olup olmadığını kontrol eden işlev
function areCheckboxesSelected() {
    var inwiCheckbox = $("#flexCheckDefault").is(":checked");
    var orangeCheckbox = $("#flexCheckChecked").is(":checked");

    return inwiCheckbox || orangeCheckbox;
}

// Dosya yükleme işlemini gerçekleştiren işlev
function handleFile(file, isOrange) {
    var formData = new FormData();
    formData.append("file", file);

    var url = isOrange ? "@Url.Action("UploadExcelOrange", "Home")" : "@Url.Action("UploadExcelInwi", "Home")";

    $.ajax({
    url: url,
    type: "POST",
    data: formData,
    processData: false,
    contentType: false,
    success: function (response) {
        if (response.success) {
            var message = "Success Message";
            if (response.errorRows && response.errorRows.length > 0) {
                message += "<br>Number of incorrect rows: " + response.errorRows.length + "<br>Error rows: " + response.errorRows.join(" , ");
            }
            showPopupMessageOrange(message, true, response.errorRows);
        } else {
            console.log(response.ErrorMessage); // Hata mesajını console.log ile yazdır
            alert("Error occurred. Please check the console for details."); // Kullanıcıya genel bir hata mesajı göster
        }
    },  error: function (xhr, status, error) {
            // Sunucudan gelen hata mesajını tarayıcı konsolunda göster
            console.log("An error occurred: " + error.responseText);

            // Kullanıcıya hata mesajını göster
            alert("An error occurred. Please check the console for details.\n\nError Message: " + error.responseText);
        }
});

}

// Başarı veya hata mesajlarını gösteren işlev
        function showPopupMessageOrange(message, success, errorRows) {
            var alertDiv = $("#alertmessageSIM");
            var alertIcon = success ? '<i class="fe fe-thumbs-up"></i>' : '<i class="fe fe-alert-circle"></i>';

            alertDiv.html('<div class="alert ' + (success ? 'alert-success' : 'alert-danger') + '" role="alert">' +
                '<span class="alert-inner--icon">' + alertIcon + '</span>' +
                '<span class="alert-inner--text"><strong>' + (success ? 'Success!' : 'Error!') + '</strong> ' + message + '</span>' +
                '</div>');

            alertDiv.show();

            if (errorRows && errorRows.length > 0) {
                var errorList = $("#errorListSIM");
                errorList.empty();

                errorRows.forEach(function (errorRow) {
                    var listItem = document.createElement("li");
                    listItem.textContent = "Satır " + errorRow;
                    errorList.append(listItem);
                });

                var errorRowsContainer = $("#errorRowsSIM");
                errorRowsContainer.show();
            } else {
                var errorRowsContainer = $("#errorRowsSIM");
                errorRowsContainer.hide();
            }
        }

// Yükle düğmesine tıklanınca çalışacak işlev
        $("#btnUploadSIM").click(function () {
            var inwiCheckbox = $("#flexCheckDefault").is(":checked");
            var orangeCheckbox = $("#flexCheckChecked").is(":checked");

            if (inwiCheckbox !== orangeCheckbox) {
                // Hem Inwi hem de Orange seçilmediyse işlem yapma
                var fileInput = $("#fileSIM")[0];
                var file = fileInput.files[0];

                if (file) {
                    if (inwiCheckbox) {
                        // Inwi checkbox seçili ise Inwi için yükleme yap
                        handleFile(file, false, function (inwiResponse) {
                            // Inwi yükleme işlemi tamamlandığında buraya gelir
                            // inwiResponse değişkeni Inwi yükleme işleminin sonucunu içerir
                            if (inwiResponse.success) {
                                // Inwi yükleme başarılı ise burada işlem yapabilirsiniz
                            } else {
                                // Inwi yükleme başarısız ise burada hata mesajını gösterin
                                showPopupMessageOrange("Inwi upload error: " + inwiResponse.message, false, inwiResponse.errorRows);
                            }
                        });
                    }
                    if (orangeCheckbox) {
                        // Orange checkbox seçili ise Orange için yükleme yap
                        handleFile(file, true, function (orangeResponse) {
                            // Orange yükleme işlemi tamamlandığında buraya gelir
                            // orangeResponse değişkeni Orange yükleme işleminin sonucunu içerir
                            if (orangeResponse.success) {
                                // Orange yükleme başarılı ise burada işlem yapabilirsiniz
                            } else {
                                // Orange yükleme başarısız ise burada hata mesajını gösterin
                                showPopupMessageOrange("Orange upload error: " + orangeResponse.message, false, orangeResponse.errorRows);
                            }
                        });
                    }
                } else {
                    showPopupMessageOrange("@Resources.L341", false);
                }
            } else {
                showPopupMessageOrange("@Resources.L340", false);
            }
        });

// Diğer olay dinleyicileri ve işlevleri burada tanımlanabilir.
    </script>

