    <script>
 document.addEventListener("DOMContentLoaded", function () {
         var fileInputAcc = document.getElementById("fileAcc");
         var browseButtonAcc = document.getElementById("btnBrowseAcc");
         var cancelButtonAcc = document.getElementById("btnCancelAcc");
         var uploadButtonAcc = document.getElementById("btnUploadAcc");
         var fileDropAreaAcc = document.querySelector(".file-drop-areaAcc");
         var uploadedFileNameAcc = document.getElementById("uploadedFileNameAcc");

         fileDropAreaAcc.addEventListener("dragover", function (e) {
             e.preventDefault();
             e.stopPropagation();
             fileDropAreaAcc.classList.add("dragover");
         });

         fileDropAreaAcc.addEventListener("dragleave", function (e) {
             e.preventDefault();
             e.stopPropagation();
             fileDropAreaAcc.classList.remove("dragover");
         });

         fileDropAreaAcc.addEventListener("drop", function (e) {
             e.preventDefault();
             e.stopPropagation();
             fileDropAreaAcc.classList.remove("dragover");

             var file = e.dataTransfer.files[0];
             uploadedFileNameAcc.innerText = file.name;
             fileInputAcc.files = e.dataTransfer.files;
         });

         browseButtonAcc.addEventListener("click", function () {
             fileInputAcc.click();
         });
         fileInputAcc.addEventListener("change", function () {
             uploadedFileNameAcc.innerText = fileInputAcc.files[0].name;
         });

         uploadButtonAcc.addEventListener("click", function () {
             var file = fileInputAcc.files[0];
             if (file) {
                 handleFileAcc(file);
             } else {
                 alert("@Resources.L281");
             }
         });
         cancelButtonAcc.addEventListener("click", function () {
             // Dosya seçimi silme işlemi
             fileInputAcc.value = null; // Dosya seçimi sıfırlanıyor
             uploadedFileNameAcc.innerText = ""; // Dosya adını temizle
             location.reload();
         });
     });

